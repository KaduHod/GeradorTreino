
<head>
  <style>
    body{
      background-image: url('/img/Treino.jpg');
      background-size:cover;
      background-position: center;
    }
    .containerTreino{
      display: flex;
      width: 100%;
      border-radius: 10px;
      padding: 10px;
      justify-content: center;
      padding-bottom: 150px;
    }
    .containerTreinoItem{
      width: 90%;
    }
  </style>
</head>
<div class="containerTreino">
  
  
  <div class="containerTreinoItem">
    <table class="table table-dark">
  <tr>
    {{#each nomesMusculos}}
    <th>
      {{this}}
    </th>
    {{/each}}
  </tr>
  <tr>
    {{#each linha1}}
    <td>
      {{this}}
    </td>
    {{/each}}
  </tr>
  <tr>
    {{#each linha2}}
    <td>
      {{this}}
    </td>
    {{/each}}
  </tr>
  <tr>
    {{#each linha3}}
    <td>
      {{this}}
    </td>
    {{/each}}
  </tr>
  <tr>
    {{#each linha4}}
    <td>
      {{this}}
    </td>
    {{/each}}
  </tr>
    </table>
    <form action="/treinos/" method="post">
        {{#each dados}}
        <input type="hidden" name="{{@key}}" value="{{this}}">
        {{/each}}
        <input type="submit" class="btn btn-dark" value="Voltar">
    </form>
  </div>

</div>


