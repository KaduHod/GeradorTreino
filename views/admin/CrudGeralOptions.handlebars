        function removeTodosOsFilhos(div){
           while(div.childElementCount > 0){
               div.childNodes.forEach((filho)=>{
                   div.removeChild(filho)
               })
           }
       }
       
        function tiraText(nodeList){
                    filhosFiltrados = []
                    cont = 0
                    nodeList.forEach((el)=>{
                        if(cont%2!=0){
                            filhosFiltrados.push(el)
                        }
                        cont++
                    })
                    return filhosFiltrados
                }
        
