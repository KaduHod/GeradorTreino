        function verificaOption1Cross(){
            var botao = document.getElementById('botao_')
            var valorInput1 = document.getElementById('input_primeira_opcao_geral')
            var valorInput1Value = valorInput1.value
            if(valorInput1Value == 'Mostrar todos os exercicios'){
                botao.disabled = false
                botao.setAttribute('onclick','mostraTodosOsExerciciosCross()')
            }else{
                var CrossInputs1 = document.getElementById('CrossInputs1')
                if(valorInput1Value == 'Categoria'){
                    console.log('Mostra por cat')
                    // tenho que formatar o input para que tenha categorias Resistencia, Potencia e isometria
                    console.log(CrossInputs1.childNodes)
                    if(CrossInputs1.childElementCount != null){

                        CrossInputs1.childNodes.forEach((filho)=>{
                        CrossInputs1.removeChild(filho)
                        })

                        var selectCategorias = document.createElement('select')
                        selectCategorias.setAttribute('id','select_para_categorias')
                        selectCategorias.setAttribute('class','form-control')
                        var arrCats = ['Resistencia','Potencia','Isometria']
                        arrCats.forEach((categoria)=>{
                            var optionCat = document.createElement('option')
                            optionCat.setAttribute('id',`${categoria}OPT`)
                            optionCat.setAttribute('value',`${categoria}`)
                            optionCat.innerHTML = `${categoria}`
                            selectCategorias.appendChild(optionCat)

                        })
                        CrossInputs1.appendChild(selectCategorias)
                    }else{
                        console.log('todos os filhos ja removidos')
                    }
                    botao.setAttribute('onclick','mostra_por_categoria()')
                    botao.disabled = false;
                    var div_prossiga = document.getElementById('botaoProssiga')
                    div_prossiga.setAttribute('onclick','mostra_por_categoria()')
                }

                if(valorInput1Value == 'Parte do corpo'){
                    // criar inputs com seleção porparte do corpo Membro superior, membro inferior, multi-articular e core
                    var crossInput = document.querySelector('#input_primeira_opcao_geral')
                    while(crossInput.childElementCount > 0){
                        crossInput.childNodes.forEach((filho)=>{
                            crossInput.removeChild(filho)
                        })
                    }
                    arrOptPCR = ['Membro superior','Membro inferior','Core','Multi-articular']
                    arrOptPCR.forEach((el)=>{
                        var optionPCR = document.createElement('option')
                        optionPCR.setAttribute('id',`optionPCR-${el}`)
                        optionPCR.setAttribute('value',`${el}`)
                        optionPCR.setAttribute('class','form-control')
                        optionPCR.innerHTML = `${el}`
                        crossInput.appendChild(optionPCR)
                    })
                    botao.setAttribute('onclick','mostra_por_PCR()')
                    botao.disabled = false;
                    var div_prossiga = document.getElementById('botaoProssiga')
                    div_prossiga.setAttribute('onclick','mostra_por_PCR()')
                }
            
                if(valorInput1Value == 'Equipamento'){
                    var crossInput = document.querySelector('#input_primeira_opcao_geral')
                    while(crossInput.childElementCount > 0){
                        crossInput.childNodes.forEach((filho)=>{
                            crossInput.removeChild(filho)
                        })
                    }
                    arrOptEquip = ['Peso corporal','Com equipamento']
                    arrOptEquip.forEach((el)=>{
                        var optionEquip = document.createElement('option')
                        optionEquip.setAttribute('class','form-control')
                        optionEquip.setAttribute('value',`${el}`)
                        optionEquip.setAttribute('id',`optionEquip-${el}`)
                        optionEquip.innerHTML = `${el}`
                        crossInput.appendChild(optionEquip)
                    })
                    botao.setAttribute('onclick','mostra_por_Equip()')
                    botao.disabled = false;
                    var div_prossiga = document.getElementById('botaoProssiga')
                    div_prossiga.setAttribute('onclick','mostra_por_Equip()')
                }
            }
        }
        
